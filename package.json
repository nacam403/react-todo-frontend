{
  "name": "react-todo-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "npm run watch",
    "test": "mocha --require babel-register ./test/**/*.test.js",
    "clean:src-assets": "rimraf ./src/assets/*.js ./src/assets/*.html && mkdirp ./src/assets",
    "clean:dist": "rimraf ./dist && mkdirp ./dist",
    "clean": "run-p clean:*",
    "lint:js": "eslint --fix ./src/js/**/*.js",
    "prebuild:js": "run-p clean:src-assets lint:js",
    "build:js": "webpack --config webpack.develop.babel.js",
    "build-production:js": "webpack --define process.env.NODE_ENV=\"'production'\" --config webpack.production.babel.js",
    "watch:js": "webpack --config webpack.develop.babel.js --watch",
    "watch:server": "browser-sync start --serveStatic src/assets --files src/assets --proxy localhost:9000",
    "watch": "npm-run-all build:js -p watch:*",
    "release:copy": "cpx './src/assets/**/*' ./dist",
    "release": "npm-run-all clean release:copy build-production:js"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "browser-sync": "^2.18.13",
    "compression-webpack-plugin": "^1.0.1",
    "cpx": "^1.5.0",
    "css-loader": "^0.28.7",
    "eslint": "^4.8.0",
    "eslint-plugin-flowtype": "^2.41.0",
    "eslint-plugin-react": "^7.4.0",
    "fetch-mock": "^5.13.1",
    "flow-bin": "^0.63.1",
    "html-webpack-plugin": "^2.30.1",
    "mkdirp": "^0.5.1",
    "mocha": "^4.0.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "power-assert": "^1.4.4",
    "redux-devtools": "^3.4.1",
    "redux-mock-store": "^1.3.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "uglify-save-license": "^0.4.1",
    "uglifyjs-webpack-plugin": "^1.0.1",
    "webpack": "^3.10.0",
    "webpack-merge": "^4.1.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-intl": "^2.4.0",
    "react-intl-redux": "^0.7.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  }
}
