{
  "name": "npm-script-study",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "npm run watch",
    "test": "mocha --require babel-register ./test/**/*.test.js",
    "clean": "rimraf ./dist && mkdirp ./dist",
    "lint:js": "eslint --fix ./src/js/**/*.js",
    "prebuild:js": "npm run lint:js",
    "build:js": "webpack",
    "build-production:js": "webpack --define process.env.NODE_ENV=\"'production'\"",
    "watch:js": "webpack --watch",
    "watch:server": "browser-sync start --serveStatic src/assets --files src/assets --proxy localhost:9000",
    "watch": "run-p watch:*",
    "compress:js": "uglifyjs ./src/assets/bundle.js --compress --mangle --comments --output ./dist/bundle.js",
    "release:js": "run-s build-production:js compress:js",
    "release:copy": "cpx './src/assets/**/*' ./dist",
    "release": "npm-run-all clean release:copy -p release:js"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "browser-sync": "^2.18.13",
    "cpx": "^1.5.0",
    "eslint": "^4.8.0",
    "css-loader": "^0.28.7",
    "eslint-plugin-react": "^7.4.0",
    "fetch-mock": "^5.13.1",
    "mkdirp": "^0.5.1",
    "mocha": "^4.0.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "power-assert": "^1.4.4",
    "redux-devtools": "^3.4.0",
    "redux-mock-store": "^1.3.0",
    "rimraf": "^2.6.2",
    "uglify-js": "^3.1.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  }
}
